{
  "name": "web",
  "version": "1.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "fix": "yarn lint:fix && yarn prettier:fix",
    "clean": "lerna exec --parallel -- rimraf dist node_modules/.cache",
    "lint": "eslint ./ --ext js,ts",
    "lint:fix": "eslint ./ --ext js,ts --fix",
    "prettier": "prettier --check ./**/*.{js,ts}",
    "prettier:fix": "prettier --write ./**/*.{js,ts}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaeltec/web.git"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "@hitechline/eslint-config-web": "1.0.2",
    "@types/node": "14.14.8",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.8",
    "eslint": "7.11.0",
    "lerna": "3.22.1",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "typescript": "4.0.3"
  }
}
